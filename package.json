{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "pocket",
  "author": "Danial Raza <danialrazafb@gmail.com>",
  "main": "dist/index.js",
  "private": true,
  "license": "Apache-2.0",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "commitlint": "commitlint --edit",
    "deploy": "pnpm build && node --env-file=.env dist/deploy.js",
    "dev": "pnpm build && pnpm start",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint --fix src",
    "lint:check": "eslint src",
    "prepare": "husky",
    "start": "node --env-file=.env dist/index.js"
  },
  "dependencies": {
    "discord.js": "^14.14.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.0.3",
    "@commitlint/config-angular": "^19.0.3",
    "@types/node": "^20.11.25",
    "eslint": "^8.57.0",
    "eslint-config-neon": "^0.1.59",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "is-ci": "^3.0.1",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "typescript": "^5.4.2",
    "zod": "^3.22.4"
  },
  "engines": {
    "node": ">=20.6.0"
  }
}
