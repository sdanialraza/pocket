    "lint": "eslint --fix src",
    "lint:check": "eslint src",
    "prepare": "is-ci || husky"
  },
  "dependencies": {
    "@discordjs/core": "^1.2.0",
    "@discordjs/rest": "^2.3.0",
    "tweetnacl": "^1.0.3"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240502.0",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-angular": "^19.3.0",
    "@types/node": "^20.12.8",
    "eslint": "^9.26.0",
    "eslint-config-neon": "^0.1.62",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "is-ci": "^3.0.1",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "typescript": "^5.4.5",
    "wrangler": "^3.114.17"
  },
  "engines": {
    "node": ">=20.6.0"
  }
}
