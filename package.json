{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "pocket",
  "author": "Danial Raza <danialrazafb@gmail.com>",
  "main": "dist/index.js",
  "private": true,
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "commitlint": "commitlint --edit",
    "deploy": "pnpm build && wrangler deploy",
    "dev": "wrangler dev src/index.js",
    "format": "prettier --write .",
